@model IEnumerable<AplicativoPedido.Models.Attach>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.labelUPC.nameUPC)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.labelUPC.nameUPC)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.attachID }) |
            @Html.ActionLink("Details", "Details", new { id=item.attachID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.attachID })
        </td>
    </tr>
}

</table>
