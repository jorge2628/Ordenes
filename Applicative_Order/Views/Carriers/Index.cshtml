@model IEnumerable<Applicative_Order.Models.Carrier>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.codCarrier)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nameCarrier)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.codCarrier)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nameCarrier)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idCarrier }) |
            @Html.ActionLink("Details", "Details", new { id=item.idCarrier }) |
